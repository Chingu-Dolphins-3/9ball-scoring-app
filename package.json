{
  "name": "Break-n-Score",
  "version": "0.1.0",
  "private": true,
  "author": "William Hurley",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/platform-express": "^6.5.3",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "24.0.11",
    "@types/lodash": "^4.14.137",
    "@types/node": "11.13.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^6.0.9",
    "dotenv": "^8.1.0",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "semantic-ui-react": "^0.83.0",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.3",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/supertest": "2.0.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "react-scripts-ts": "3.1.0",
    "react-test-renderer": "^16.6.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "^8.3.0",
    "tsc-watch": "^2.4.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "scripts": {
    "client:build": "react-scripts-ts build",
    "client:start": "react-scripts-ts start",
    "client:test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "format": "prettier --write \"server/src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "server:build": "rimraf server/dist && tsc -p tsconfig.build.json --outDir server/dist",
    "server:start": "ts-node -r tsconfig-paths/register server/src/main.ts",
    "server:start:debug": "tsc-watch -p tsconfig.build.json --outDir ./server/dist --onSuccess \"node --inspect-brk server/dist/main.js\"",
    "server:start:dev": "tsc-watch -p tsconfig.build.json --outDir ./server/dist --onSuccess \"node server/dist/main.js\"",
    "server:start:prod": "node server/dist/main.js",
    "server:test": "jest",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "jest --config ./server/test/jest-e2e.json",
    "server:test:watch": "jest --watch"
  },
  "proxy": "http://localhost:5000"
}
